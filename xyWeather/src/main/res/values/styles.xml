<resources>

	<!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.








    -->
	<style name="AppBaseTheme" parent="android:Theme.Light">
		<!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.








        -->
	</style>

	<!-- Base application theme. -->
	<style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
		<!-- Customize your theme here. -->
		<item name="colorPrimary">@color/colorPrimary</item>
		<item name="colorPrimaryDark">@color/colorPrimaryDark</item>
		<item name="colorAccent">@color/colorAccent</item>
	</style>

	<style name="loadingProgressStyle">
		<item name="android:maxWidth">88dip</item>
		<item name="android:maxHeight">88dip</item>
		<item name="android:indeterminateDrawable">@anim/circle_loading</item>
		<item name="android:minWidth">88dip</item>
		<item name="android:minHeight">88dip</item>
	</style>

	<style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

	<style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>